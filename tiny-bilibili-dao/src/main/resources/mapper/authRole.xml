<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moon.tinybilibili.dao.AuthRoleDao">

    <select id="getRoleByCode" parameterType="java.lang.String" resultMap="com.moon.tinybilibili.domain.auth.AuthRole">
        select *
        from t_auth_role
        where code = #{code}
    </select>
</mapper>